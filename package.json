{
  "name": "ultimate-luchador",
  "version": "1.0.0",
  "description": "Final project - Matt, Justin, Gavin",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "docronjob": "node cron-job.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gavbax99/ultimate-luchador.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gavbax99/ultimate-luchador/issues"
  },
  "homepage": "https://github.com/gavbax99/ultimate-luchador#readme",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "@google-cloud/storage": "^2.3.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "firebase": "^5.5.8",
    "if-env": "^1.0.4",
    "multer": "^1.4.1",
    "mysql": "^2.16.0",
    "react-dom": "^16.6.0",
    "react-dropzone": "^7.0.1",
    "react-images-upload": "^1.2.3"
  }
}
